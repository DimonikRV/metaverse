
 
  {
    "env": { 
        "browser": true,
        "es2021": true 
    },
    "extends": [ 
        "next/core-web-vitals",
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime",
        "plugin:react-hooks/recommended",
        "plugin:jsx-ally/recommended",
        "plugin:import/recommended",
        "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react", 
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "react/prop-types": 0 
    }
}

